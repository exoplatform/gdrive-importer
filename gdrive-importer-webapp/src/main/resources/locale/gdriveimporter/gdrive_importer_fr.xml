<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<bundle>
    <UITabForm>
        <label>
            <cloneGDrive>Cloner depuis Google Drive</cloneGDrive>
        </label>
    </UITabForm>
    <UIActionBar>
        <tooltip>
            <CloneGDrive>Cloner depuis votre Google Drive</CloneGDrive>
        </tooltip>
    </UIActionBar>
    <UIPopupWindow>
        <title>
            <UICloudLinkForm>Cloner depuis Drive</UICloudLinkForm>
        </title>
    </UIPopupWindow>
    <UICloudLinkForm>
        <action>
            <Cancel>Cancel</Cancel>
            <Clone>connecter et cloner votre google drive</Clone>
        </action>
        <label>
            <titleTextBox>Folder Or File Link in Google Drive:</titleTextBox>
            <customTypeCheckBox>Use a custom type of folder</customTypeCheckBox>
        </label>
        <title>Clone Folders/Files From Drive</title>
        <msg>
            <folder.cloned>Folder has been successfully cloned</folder.cloned>
            <folder.cloned>Si vous ne spécifiez pas de lien, toutes les données du lecteur seront clonées.</folder.cloned>
        </msg>
        <help/>
    </UICloudLinkForm>
</bundle>